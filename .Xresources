rofi.font: RobotoMono 20
URxvt.font: xft:RobotoMono:style=Regular:size=12:antialias=true,xft:FreeMono:size=12
URxvt.boldFont: xft:RobotoMono:style=Bold:size=12,xft:FreeMono:size=12
URxvt.italicFont: xft:RobotoMono:style=Italic:size=12
URxvt.boldItalicFont: xft:RobotoMono:style=Bold Italic:size=12
! URxvt.font:        -xos4-terminus-medium-*-*-*-32-*-*-*-*-*-*-*
! URxvt.boldFont:    -xos4-terminus-bold-*-*-*-32-*-*-*-*-*-*-*
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.internalBorder: 40
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 10000
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: false
URxvt.depth: 30
URxvt.iso14755: false

!! Utils
URxvt.perl-ext-common: ...,clipboard,keyboard-select,resize-font
! copy paste
URxvt.clipboard.autocopy: true
URxvt.keysym.Mod4-c: perl:clipboard:copy
URxvt.keysym.Mod4-v: perl:clipboard:paste
! search and copy
URxvt.keysym.Mod4-Escape: perl:keyboard-select:activate
URxvt.keyboard-select.clipboard: true
! resize font
URxvt.keysym.C-minus: resize-font:smaller
URxvt.keysym.C-equal: resize-font:bigger
URxvt.keysym.C-0: resize-font:reset
