pointer = 1 2 3 5 4 7 6 8 9 10 11 12

! assign control to caps
clear lock
clear control
keycode 66 = Control_L
add control = Control_L Control_R

! swap super and alt
clear mod1
clear mod4

keycode 133 = Alt_L
keycode 64 = Super_L

add mod1 = Alt_L Alt_R Meta_L
add mod4 = Super_L Super_R Super_L Hyper_L

